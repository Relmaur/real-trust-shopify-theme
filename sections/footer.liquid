    <div class="column-2 star">
      <svg width="92" height="112" viewBox="0 0 92 112" fill="none" xmlns="http://www.w3.org/2000/svg">
        <path fill-rule="evenodd" clip-rule="evenodd"
            d="M46 0C46 30.928 25.4051 56 0 56C25.4051 56 46 81.072 46 112C46 81.072 66.5949 56 92 56C66.5949 56 46 30.928 46 0Z"
            fill="#363B3E" />
      </svg>
    </div>
    <div class="column-3">
      <div class="sub_column-1">
        {% assign services_link = linklists['rt-menu'].links | where: 'title', 'Services' | first %}
        <a href="{{ services_link.url }}">Services</a>
        {% if services_link.links %}
          <ul>
            {% for childLink in services_link.links %}
              <li>
                <a href="{{ childLink.url }}">{{ childLink.title }}</a>
              </li>
            {% endfor %}
          </ul>
        {% endif %}
      </div>
      <div class="sub_column-2">
        {% assign about_link = linklists['rt-menu'].links | where: 'title', 'About Projects' | first %}
        <a href="{{ about_link.url }}">About Projects</a>
        {% if about_link.links %}
          <ul>
            {% for childLink in about_link.links %}
              <li>
                <a href="{{ childLink.url }}">{{ childLink.title }}</a>
              </li>
            {% endfor %}
          </ul>
        {% endif %}
      </div>
      <div class="sub_column-3">
        {% assign testimonials_link = linklists['rt-menu'].links | where: 'title', 'Testimonials' | first %}
        <a href="{{ testimonials_link.url }}">Testimonials</a>
        {% if testimonials_link.links %}
          <ul>
            {% for childLink in testimonials_link.links %}
              <li>
                <a href="{{ childLink.url }}">{{ childLink.title }}</a>
              </li>
            {% endfor %}
          </ul>
        {% endif %}
      </div>
    </div>
