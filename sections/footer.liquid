      "type": "text",
      "id": "footer_social_media_1_url",
      "label": "Facebook URL",
      "default": "#"
    },
    {
      "type": "text",
      "id": "footer_social_media_2_url",
      "label": "Linkedin URL",
      "default": "#"
    },
    {
      "type": "text",
      "id": "footer_social_media_3_url",
      "label": "Pinterest URL",
      "default": "#"
    },
    {
      "type": "text",
      "id": "footer_social_media_4_url",
      "label": "Instagram URL",
      "default": "#"
    }
  ]
}
{% endschema %}
